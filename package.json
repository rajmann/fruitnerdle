    "lucide-react": "^0.453.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/node": "20.16.11",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3",
    "vite": "^5.4.14",
    "tsx": "^4.19.1"
